{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": ".",
        "rootDir": ".",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.ts"
    ]
}
